        v = ds_list_find_value(player_cards, ds_list_size(player_cards) - 1);
        v.value = choose(cards.archer, cards.warrior, cards.mage);
        v.angle = 180;   
    }
    v.moved = true;
    v.point_x[0] = 150 + t;
    v.point_y[0] = 500;
    
    t += 120;
}
//One of the cards is changed by trump
