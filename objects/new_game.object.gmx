          <argument>
            <kind>1</kind>
            <string>///Check round

if(global.played_card_oldman != 0
    and global.played_card_player != 0
    and game_round)    
{
    
    scale = 2;//Icon scaling
    
    var scores = 0;
    //scores = 1 -&gt; win
    //      = -1 -&gt; lose
    //      = 0 -&gt; status quo
    
    // Archer beats warrior
    // Mage beats archer
    // Warrior beats mage
    //Super beats all exclude super enemy
    //(Super-mage can be beaten by super_warrior, etc)
    
    switch(global.played_card_player.value)
    {
    
        case cards.archer:
        {
            scores = global.played_card_oldman.value - 1;
            if(scores == 0)
                scores -= 1;
            else if(scores == -1)
                scores += 1;
            if(global.played_card_oldman.value &gt;= 3)
                scores = -1;    
            break;
        }
        
        case cards.mage:
        {
            scores = global.played_card_oldman.value - 1;
            if(global.played_card_oldman.value &gt;= 3)
                scores = -1;
            else    
                scores = -scores;
            break;
        }
        
        case cards.warrior:
        {
            scores = global.played_card_oldman.value - 1;
            if(scores == 1)
                scores = 0;
            else if(scores == 0)
                scores = 1;
            if(global.played_card_oldman.value &gt;= 3)
                scores = -1;
            break;
        }
        
        case cards.super_archer:
        {
            scores = global.played_card_oldman.value;
            if(scores == cards.super_mage)
                scores = -1;
            else if(scores == cards.super_archer)
                scores = 0;
            else
                scores = 1;
        }
        
        case cards.super_mage:
        {
            scores = global.played_card_oldman.value;
            if(scores == cards.super_warrior)
                scores = -1;
            else if(scores == cards.super_mage)
                scores = 0;
            else
                scores = 1;
        }
        
        case cards.super_warrior:
        {
            scores = global.played_card_oldman.value;
            if(scores == cards.super_archer)
                scores = -1;
            else if(scores == cards.super_warrior)
                scores = 0;
            else
                scores = 1;
        }
    }
    
    winner = scores;
    total_score += winner;
    
    if(winner == -1)
        winner = 2;// 2 is oldman, 1 is player, 0 - noone
    
    game_round = false;
    alarm[1] = room_speed * 3;

    rounds++;    
}
