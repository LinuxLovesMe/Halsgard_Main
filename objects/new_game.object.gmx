        if(i &lt; 2)
            v.value = cards.archer;
        if(i &lt; 4 and i &gt;= 2)
            v.value = cards.mage;
        if(i &lt; 6 and i &gt;= 4)
            v.value = cards.warrior;
        v.angle = 180;
    }
    else
    {
        temp = instance_create(200 + t, -500, oldman_card);
