            <kind>1</kind>
            <string>event_inherited();

var t = ds_list_size(insides);

if(searched and t &gt; 0)
{
    var topleft_x = view_xview[0] + 400;
    var topleft_y = view_yview[0] + 200;
    
    draw_rectangle_colour(
        topleft_x, topleft_y,
        topleft_x + 500, topleft_y + 50 * t,
        c_black, c_black, c_black, c_black, false);
    
    var offset = 0;
    for(var i = 0; i &lt; ds_list_size(insides); i++)
    {
        t = ds_list_find_value(insides, i);
        scalex = 40 / t.sprite_width;
        scaley = 40 / t.sprite_height;
        
        draw_sprite_ext(t.sprite_index, 0,
           topleft_x + (t.sprite_width * scalex) / 2, 
           topleft_y + offset + (t.sprite_height * scaley) / 2,
            scalex, scaley, 0, c_white, 1);
        
        draw_set_colour(c_white); 
        draw_text_ext(topleft_x + (t.sprite_width * scalex) + 10, 
           topleft_y + offset + (t.sprite_height * scaley) / 2,
           t.name, 20, 500);
           
        if(point_in_rectangle(mouse_x, mouse_y,
            topleft_x,
            topleft_y + offset,
            topleft_x + 450,
            topleft_y + offset + 50))
        {
            if(mouse_check_button_pressed(mb_left))
            {
                for(var j = 0; j &lt; ds_list_size(global.inventory); j++)
                {
                    if(ds_list_find_value(global.inventory, j) == 0)
                    {
                       ds_list_replace(global.inventory, j, t);
                       ds_list_delete(insides, i);   
                       break;
                   }
                }
                t.taken = true;
            }
        }
        
        offset += 50;
    }
    
    draw_sprite(spr_cross, 0, 
        topleft_x + 500 - sprite_get_width(spr_cross) / 2, 
        topleft_y + sprite_get_height(spr_cross) / 2);
        
    if(point_in_rectangle(mouse_x, mouse_y,
        topleft_x + 500 - sprite_get_width(spr_cross) / 2,
        topleft_y - sprite_get_height(spr_cross) / 2,
        topleft_x + 500 + sprite_get_width(spr_cross) / 2,
        topleft_y + sprite_get_height(spr_cross) / 2))
    {
        if(mouse_check_button_pressed(mb_left))
            searched = false;
    }
}
</string>
